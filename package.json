{
  "name": "@robinblomberg/schema",
  "version": "0.2.2",
  "author": "Robin Blomberg",
  "type": "module",
  "main": "lib/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@robinblomberg/eslint-config-robinblomberg": "^0.2.20",
    "@robinblomberg/test": "^0.5.7",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3"
  },
  "scripts": {
    "compile": "rimraf dist && tsc",
    "dev": "nodemon --watch . --exec \"concurrently \\\"node ./lib/index.test.js\\\" tsc\"",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "pre-commit": "concurrently --kill-others-on-fail \"npm run test\" eslint",
    "test": "node ./lib/index.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run compile && npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "cross-env CI=true eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBlomberg/schema.git"
  },
  "bugs": {
    "url": "https://github.com/RobinBlomberg/schema/issues"
  },
  "homepage": "https://github.com/RobinBlomberg/schema#readme"
}
